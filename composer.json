{
  "name": "psx/schema",
  "description": "Parse and generate data schema formats",
  "keywords": ["schema", "json", "jsonschema"],
  "homepage": "http://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com/"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/annotations": "^1.3",
    "nikic/php-parser": "^4.0",
    "psr/cache": "^1.0",
    "psx/cache": "^1.0",
    "psx/datetime": "^1.0",
    "psx/http": "^2.0",
    "psx/json": "^1.0",
    "psx/record": "^1.0",
    "psx/uri": "^1.0",
    "psx/validate": "^1.0",
    "symfony/console": "^4.0|^5.0",
    "phpdocumentor/type-resolver": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "psx/model": "^2.0",
    "vimeo/psalm": "^3.11"
  },
  "bin": ["bin/schema"],
  "autoload": {
    "psr-4": {
      "PSX\\Schema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Schema\\Tests\\": "tests/"
    }
  }
}
