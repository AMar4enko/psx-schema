{
  "definitions": {
    "Author": {
      "description": "An simple author element with some description",
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "email": {
          "description": "We will send no spam to this address",
          "nullable": true,
          "type": "string"
        },
        "categories": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "locations": {
          "description": "Array of locations",
          "type": "array",
          "items": {
            "$ref": "Location"
          }
        },
        "origin": {
          "$ref": "Location"
        }
      }
    },
    "Location": {
      "description": "Location of the person",
      "type": "object",
      "properties": {
        "lat": {
          "type": "number"
        },
        "long": {
          "type": "number"
        }
      }
    },
    "Meta": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "Web": {
      "description": "An application",
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "News": {
      "description": "An general news entry",
      "type": "object",
      "properties": {
        "config": {
          "$ref": "Meta"
        },
        "inlineConfig": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "mapReceiver": {
          "additionalProperties": {
            "$ref": "Author"
          },
          "type": "object"
        },
        "mapTags": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "receiver": {
          "type": "array",
          "items": {
            "$ref": "Author"
          }
        },
        "profileImage": {
          "format": "base64",
          "type": "string"
        },
        "read": {
          "type": "boolean"
        },
        "author": {
          "$ref": "Author"
        },
        "meta": {
          "$ref": "Meta"
        },
        "sendDate": {
          "format": "date",
          "type": "string"
        },
        "readDate": {
          "format": "date-time",
          "type": "string"
        },
        "price": {
          "type": "number"
        },
        "rating": {
          "type": "integer"
        },
        "content": {
          "description": "Contains the main content of the news entry",
          "type": "string"
        },
        "question": {
          "type": "string"
        },
        "version": {
          "type": "string"
        },
        "coffeeTime": {
          "format": "time",
          "type": "string"
        },
        "g-recaptcha-response": {
          "type": "string"
        },
        "media.fields": {
          "type": "string"
        },
        "payload": {
          "type": "any"
        }
      }
    }
  },
  "$ref": "News"
}